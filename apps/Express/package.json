{
  "name": "@asepharyana/express",
  "version": "1.0.0",
  "description": "Express application for Asepharyana",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "start": "node dist/cjs/index.js",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir dist/cjs --extensions \".ts,.tsx\"",
    "build:esm": "BABEL_ENV=esm babel src --out-dir dist/esm --extensions \".ts,.tsx\"",
    "dev": "cross-env NODE_ENV=development tsx src/index.ts",
    "format": "prettier --write .",
    "lint": "concurrently \"eslint .\" \"tsc --noEmit\"",
    "check-types": "tsc --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "prebuild": "rimraf dist && npm run generate-types",
    "generate-types": "tsc --emitDeclarationOnly --outDir dist/types"
  },
  "author": "Asepharyana",
  "license": "ISC",
  "devDependencies": {
    "@asepharyana/config-typescript": "workspace:*",
    "@asepharyana/config-eslint": "workspace:*",
    "@babel/cli": "^7.26.0",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/eslintrc": "^3.2.0",
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "eslint-config-love": "^117.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-promise": "^7.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "pretty-quick": "^4.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@asepharyana/database": "workspace:*",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "joi": "^17.13.3",
    "moment": "^2.30.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "uuid": "^11.0.5",
    "ws": "^8.18.0"
  }
}