{
  "extends": "@asepharyana/config-typescript/base.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "declaration": true,
    "noEmit": false,
    "declarationDir": "./types",
    "composite": true,
    "paths": {
      "@/*": ["./src/*"]
    },
  },
  "include": ["next-env.d.ts", "src/**/*"],
  "exclude": ["node_modules", "types","**/*.mjs",]
}
